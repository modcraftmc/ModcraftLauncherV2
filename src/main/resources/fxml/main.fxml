<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<AnchorPane fx:id="container" prefHeight="600.0" prefWidth="1000.0" styleClass="container" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.modcraftmc.launcher.ui.controllers.MainController">
   <stylesheets>
      <URL value="@../css/global.css" />
      <URL value="@../css/main.css" />
   </stylesheets>
   <children>
      <Button fx:id="close" layoutX="976.0" mnemonicParsing="false" onMouseReleased="#close" prefHeight="17.0" prefWidth="20.0" styleClass="close">
         <stylesheets>
            <URL value="@../css/global.css" />
            <URL value="@../css/main.css" />
         </stylesheets>
         <effect>
            <Blend />
         </effect></Button>
      <Button fx:id="play" layoutX="801.0" layoutY="537.0" mnemonicParsing="false" onMouseClicked="#play" prefHeight="49.0" prefWidth="185.0" styleClass="play" stylesheets="@../css/main.css" text="JOUER" />
      <Pane fx:id="accountbox" prefHeight="107.0" prefWidth="295.0" styleClass="accountbox" stylesheets="@../css/main.css">
         <children>
            <ImageView fx:id="userlogo" fitHeight="66.0" fitWidth="72.0" layoutX="14.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="https://minotar.net/avatar/manugame_" />
               </image>
            </ImageView>
            <Label fx:id="username" layoutX="91.0" layoutY="14.0" prefHeight="17.0" prefWidth="127.0" text="manugame_" textFill="WHITE">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <VBox fx:id="vboxmenu" layoutY="105.0" prefHeight="493.0" prefWidth="295.0" styleClass="vboxmenu" stylesheets="@../css/main.css">
         <children>
            <Label alignment="CENTER" prefHeight="62.0" prefWidth="281.0" text="Nos serveurs" textFill="WHITE">
               <font>
                  <Font name="Dubai Regular" size="21.0" />
               </font>
            </Label>
            <Button fx:id="server1" mnemonicParsing="false" onMouseClicked="#server1" prefHeight="58.0" prefWidth="287.0" styleClass="play" stylesheets="@../css/main.css" text="SERVEUR 1" />
            <Label prefHeight="27.0" prefWidth="281.0" />
            <Button fx:id="server2" layoutX="17.0" layoutY="79.0" mnemonicParsing="false" onMouseClicked="#server2" prefHeight="58.0" prefWidth="287.0" styleClass="play" stylesheets="@../css/main.css" text="SERVEUR 2" />
         </children></VBox>
      <Pane fx:id="hboxmenu" layoutX="297.0" layoutY="105.0" prefHeight="379.0" prefWidth="694.0" styleClass="hboxmenu" stylesheets="@../css/main.css">
         <children>
            <HBox fx:id="newsbox" layoutX="14.0" layoutY="60.0" prefHeight="259.0" prefWidth="659.0" styleClass="hbox" stylesheets="@../css/main.css" />
         </children>
      </Pane>
   </children>
</AnchorPane>
